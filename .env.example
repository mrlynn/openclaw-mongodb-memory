# Memory Daemon
MEMORY_DAEMON_PORT=7751
# MEMORY_DB_NAME=openclaw_memory   # Override database name
# MEMORY_API_KEY=your-secret-key   # Set to enable API key auth on all endpoints

# Web Dashboard (Next.js)
NEXT_PUBLIC_DAEMON_URL=http://localhost:7751

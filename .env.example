# MongoDB
MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net/openclaw_memory

# Voyage AI Embeddings
VOYAGE_API_KEY=pa-YOUR_KEY_HERE
VOYAGE_MOCK=true                   # true for mock (default), false for real embeddings
# VOYAGE_BASE_URL=                 # Leave empty for voyageai.com, or set for Atlas AI
